{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-loop-func": 1,
    "valid-jsdoc": [2, {
      "requireReturn": false,
      "requireReturnDescription": false,
      "requireParamDescription": false
    }]
  }
}
